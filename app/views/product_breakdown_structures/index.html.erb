<p id="notice"><%= notice %></p>

<h1>Listing Product Breakdown Structures</h1>

<table>
  <thead>
    <tr>
      <th>P</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_breakdown_structures.each do |product_breakdown_structure| %>
      <tr>
        <td><%= product_breakdown_structure.p_id %></td>
        <td><%= link_to 'Show', product_breakdown_structure %></td>
        <td><%= link_to 'Edit', edit_product_breakdown_structure_path(product_breakdown_structure) %></td>
        <td><%= link_to 'Destroy', product_breakdown_structure, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product breakdown structure', new_product_breakdown_structure_path %>
