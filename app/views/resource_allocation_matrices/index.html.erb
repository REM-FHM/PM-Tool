<p id="notice"><%= notice %></p>

<h1>Listing Resource Allocation Matrices</h1>

<table>
  <thead>
    <tr>
      <th>P</th>
      <th>Workpackage</th>
      <th>Component</th>
      <th>Resource</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @resource_allocation_matrices.each do |resource_allocation_matrix| %>
      <tr>
        <td><%= resource_allocation_matrix.p_id %></td>
        <td><%= resource_allocation_matrix.workpackage_id %></td>
        <td><%= resource_allocation_matrix.component_id %></td>
        <td><%= resource_allocation_matrix.resource_id %></td>
        <td><%= link_to 'Show', resource_allocation_matrix %></td>
        <td><%= link_to 'Edit', edit_resource_allocation_matrix_path(resource_allocation_matrix) %></td>
        <td><%= link_to 'Destroy', resource_allocation_matrix, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Resource allocation matrix', new_resource_allocation_matrix_path %>
