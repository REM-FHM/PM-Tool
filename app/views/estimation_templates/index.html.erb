

<h1>Listing Estimation Templates</h1>

<table>
  <thead>
    <tr>
      <th>Form template</th>
      <th>Workpackage</th>
      <th>Comment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @estimation_templates.each do |estimation_template| %>
      <tr>
        <td><%= estimation_template.form_template_id %></td>
        <td><%= estimation_template.workpackage_id %></td>
        <td><%= estimation_template.comment %></td>
        <td><%= link_to 'Show', estimation_template %></td>
        <td><%= link_to 'Edit', edit_estimation_template_path(estimation_template) %></td>
        <td><%= link_to 'Destroy', estimation_template, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Estimation template', new_estimation_template_path %>
