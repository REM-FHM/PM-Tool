

<h1>Listing Expert Forms</h1>

<table>
  <thead>
    <tr>
      <th>Round</th>
      <th>Expertname</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @expert_forms.each do |expert_form| %>
      <tr>
        <td><%= expert_form.round_id %></td>
        <td><%= expert_form.expertName %></td>
        <td><%= link_to 'Show', expert_form %></td>
        <td><%= link_to 'Edit', edit_expert_form_path(expert_form) %></td>
        <td><%= link_to 'Destroy', expert_form, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Expert form', new_expert_form_path %>
